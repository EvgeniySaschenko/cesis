							+e.INPUT(type="text" name="name" placeholder="Your name").field
							+e.I.icon.fa.fa-user
						+e.item
							+e.INPUT(type="email" name="email" placeholder="Email").field
							+e.I.icon.fa.fa-envelope
						+e.item
							+e.INPUT(type="tel" name="phone" placeholder="Phone").field
							+e.I.icon.fa.fa-phone
						+e.item
							+e.INPUT(type="text" name="text_envato_market" placeholder="Envato Market").field
							+e.I.icon.fa.fa-hospital-o
						+e.item_select
							+e.SELECT(name="looking").select
